
@{
    Layout = null;
}

<!DOCTYPE html>
<html class="no-js ss-preload" lang="en">
@Html.Action("PartialHead", "Default")

<body id="top">


    <!-- # preloader
    ================================================== -->
    <div id="preloader">
        <div id="loader">
        </div>
    </div>


    <!-- # page wrap
    ================================================== -->
    <div class="s-pagewrap">

        <div class="circles">
            <span></span>
            <span></span>
            <span></span>
            <span></span>
            <span></span>
        </div>


        <!-- ## site header
        ================================================== -->
       @Html.Action("PartialSiteHeader", "Default")
        <!-- ## main content
        ==================================================- -->
        <main class="s-content">


            <!-- ### intro
            ================================================== -->
           @Html.Action("PartialIntro", "Default")
            <!-- end s-intro -->

            <!-- ### about
            ================================================== -->
            <section id="about" class="s-about target-section">


               @Html.Action("PartialAbout", "Default")


               @Html.Action("PartialExpertise") <!-- end about-expertise -->


                <div class="row about-timelines" data-animate-block>

                    @Html.Action("PartialExperience", "Default") <!-- end column -->

                    @Html.Action("PartialEducation", "Default") <!-- end column -->


                </div> <!-- end about-timelines -->

            </section> <!-- end s-about -->
            <!-- ### works
            ================================================== -->
            <section id="works" class="s-works target-section">


              @Html.Action("PartialProject", "Default")<!-- end works-portfolio -->


             @Html.Action("PartialTestimonial", "Default")<!-- end row testimonials -->

            </section> <!-- end s-works -->
            <!-- ### contact
            ================================================== -->
            @Html.Action("PartialContact", "Default") <!-- end contact -->

        </main> <!-- end s-content -->
        <!-- ## footer
        ================================================== -->
       @Html.Action("PartialFooter", "Default")<!-- end s-footer -->

    </div> <!-- end -s-pagewrap -->
    <!-- Java Script
    ================================================== -->
  @Html.Action("PartialScripts", "Default")

</body>
</html>
